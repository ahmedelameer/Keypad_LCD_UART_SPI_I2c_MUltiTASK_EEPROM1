#include"uart.h"
#include"keypad.h"




int main(void){
    unit8 key_num;
	UART_init();
	while(1){
		 if((key_num >= 0) && (key_num <= 7))
				 {
					 UART_sendByte(key_num);
				 }
				 _delay_ms(500); //new button every 500ms
}
}
