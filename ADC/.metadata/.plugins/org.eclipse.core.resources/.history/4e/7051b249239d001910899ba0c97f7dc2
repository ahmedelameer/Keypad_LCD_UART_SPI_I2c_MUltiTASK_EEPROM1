#include"ADC.h"
#include"LCD.h"




int main(void){
	unit16 value;
	LCD_init();
	ADC_int();
	LCD_displayString("ADC value = ");


	while(1){

		LCD_gotoRawCol(0,13);

		value=ADC_readChannel(0);

		LCD_IntToStr(value);
		_delay_ms(3000);
		LCD_clearScreen();




	}






}
