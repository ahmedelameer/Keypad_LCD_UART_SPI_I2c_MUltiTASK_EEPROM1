#include"LCD.h"



int main(void){
	      LCD_init();
	     LCD_displayChar('1');
	     _delay_ms(2000);
	      LCD_clearScreen();
	      LCD_gotoRawCol(0,1);
	      LCD_displayString("ahmed");
	      LCD_gotoRawCol(0,7);
	      LCD_IntToStr(10);
	      _delay_ms(1500);
	      LCD_clearScreen();
	      LCD_displayStrRowCol(1,0,"love");
	      Direction_output(Direction_D,7);


	      while(1){
	    	  PORTD^=(1<<7);
              _delay_ms(500);
	      }




}
